    {
      "images": {
        "large": "/drivers/AC-1000/assets/images/large.png",
        "small": "/drivers/AC-1000/assets/images/small.png"
      },
      "icon": "/drivers/AC-1000/assets/icon.svg",
      "capabilities": [
        "onoff"
      ],
      "class": "socket",
      "rf": {
        "debounceTimeout": 500,
        "signal": "kakuold"
      },
      "pair": [
        {
          "id": "rf.choice",
          "options": {
            "title": "rf.pair.choice.title.switch",
            "body": "rf.pair.choice.body.switch",
            "svgHeight": "65vh",
            "choices": [
              {
                "name": "rf.pair.choice.buttons.copy.default",
                "view": "rf.imitate",
                "svg": "assets/svg/AYCT-102_pair.svg"
              },
              {
                "name": "rf.pair.choice.buttons.generate.default",
                "view": "rf.program",
                "svg": "assets/svg/pair.svg"
              }
            ]
          }
        },
        {
          "id": "rf.imitate",
          "options": {
            "title": "rf.pair.imitate.title.socket",
            "body": "rf.pair.imitate.body.socket",
            "svg": "assets/svg/AYCT-102_pair.svg"
          },
          "navigation": {
            "prev": "rf.choice"
          }
        },
        {
          "id": "rf.test_switch.2",
          "options": {
            "initWithDeviceData": true,
            "title": "rf.pair.test_switch.title.socket",
            "body": "rf.pair.test_switch.body.socket"
          },
          "navigation": {
            "prev": "rf.imitate",
            "next": "choose_slave.2"
          }
        },
        {
          "id": "choose_slave.2",
          "template": "choose_slave",
          "navigation": {
            "prev": "rf.test_switch.2",
            "next": "rf.done"
